# Root env example (server-side secrets used by API routes, backend, and scripts)

# Server-side only (do NOT prefix with NEXT_PUBLIC_)
# Preferred: AWS KMS signer (asymmetric secp256k1)
AWS_REGION=
AWS_KMS_KEY_ID=
BACKEND_API_KEY=

# Optional legacy local signer (avoid in production)
PRIVATE_KEY=
KEYSTORE_JSON=
KEYSTORE_PASSWORD=

# RPC with EIP-4844 blob support (required)
SEPOLIA_RPC_URL=

# Optional / other server-side secrets
ALCHEMY_RPC_URL=
ETHERSCAN_API_KEY=
GOOGLE_GENAI_API_KEY=
# Server Graph endpoint override used by some API routes (optional)
KAISIGN_GRAPH_ENDPOINT=

# Public (mirrors frontend .env.local; for server-rendered routes using these)
NEXT_PUBLIC_CHAIN_ID=11155111
NEXT_PUBLIC_API_URL=
NEXT_PUBLIC_KAISIGN_CONTRACT_ADDRESS=
NEXT_PUBLIC_REALITY_ETH_ADDRESS=
NEXT_PUBLIC_KAISIGN_GRAPH_URL=
NEXT_PUBLIC_REALITY_ETH_GRAPH_URL=
NEXT_PUBLIC_GTM=
NEXT_PUBLIC_ONETRUST=
NEXT_PUBLIC_IPFS_GATEWAY_URL=https://ipfs.io/ipfs

# Deployment platform can inject these instead of using .env files in prod